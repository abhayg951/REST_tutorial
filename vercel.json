{

    "version": 2,
    "builds":[
        {
            "src":"Django_RestAPI/wsgi.py",
            "use": "@vercel/python",
            "config":{ "maxLambdaSize" : "15mb", "runtime": "python3.9.13"}
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config":{
                "distDir": "static/"
        }
        }
    ],
    "routes":[
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "Django_RestAPI/wsgi.py"
        }
    ]
}